+extern uint32_t ath_ahb_freq;
+
+typedef struct {
+	int open:1, can_close:1, tmo, action;
+	wait_queue_head_t wq;
+} ath_wdt_t;
+
+static ath_wdt_t wdt_softc_array;
+
