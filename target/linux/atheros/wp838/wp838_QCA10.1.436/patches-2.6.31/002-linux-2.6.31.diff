+void HIF_PCIDeviceDetached(adf_os_handle_t os_hdl)
+{
+    /* Dereigster HIF */
+    HIF_deregister();
+
+    /* Invoke the PCI detach function for recycle resources */
+    hif_pci_detach();    
+}
