+
+void ath_gpio_init(void)
+{
+	init_MUTEX(&ath_gpio_sem);
+}
+
+void ath_gpio_down(void)
+{
+	down(&ath_gpio_sem);
+}
+
+void ath_gpio_up(void)
+{
+	up(&ath_gpio_sem);
+}
