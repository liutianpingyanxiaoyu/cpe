+	.shutdown	= ath_pci_irq_shutdown,
+	.enable		= ath_pci_irq_enable,
+	.disable	= ath_pci_irq_disable,
+	.ack		= ath_pci_irq_ack,
+	.end		= ath_pci_irq_end,
