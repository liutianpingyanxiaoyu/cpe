+	if (val & 0x1) {
+		ath_reg_rmw_set(ATH_GPIO_OUT, (1 << gpio));
+	} else {
+		ath_reg_rmw_clear(ATH_GPIO_OUT, (1 << gpio));
+	}
+}
+EXPORT_SYMBOL(ath_gpio_out_val);
