+   {                                                                        \
+      u32  temp;                                                            \
+      temp = readl(&(x)->portscx[0]);                                   \
+      temp &= ~EHCI_PORTSCX_W1C_BITS;                                       \
+      writel((temp | PORT_POWER), &(x)->portscx[0]);                    \
+   }
+   
+#define VBUS_OFF(x)                                                         \
+   {                                                                        \
+      u32  temp;                                                            \
+      temp = readl(&(x)->portscx[0]);                                   \
+      temp &= ~EHCI_PORTSCX_W1C_BITS;                                       \
+      writel ((temp & ~PORT_POWER), &(x)->portscx[0]);                  \
+   }
