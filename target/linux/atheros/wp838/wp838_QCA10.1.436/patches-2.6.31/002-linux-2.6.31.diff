+EXPORT_SYMBOL(ath_gpio_enable_slic);
+EXPORT_SYMBOL(ath_gpio_enable_uart);
+EXPORT_SYMBOL(ath_gpio_enable_stereo);
+EXPORT_SYMBOL(ath_gpio_enable_spi_cs1_cs0);
+EXPORT_SYMBOL(ath_gpio_enable_i2c_on_gpio_0_1);
