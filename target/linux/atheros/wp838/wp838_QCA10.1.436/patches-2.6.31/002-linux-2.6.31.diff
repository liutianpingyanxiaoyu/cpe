+void enable_rx_tx_intr(struct ath_pci_dev *udc)
+{
+        __u32 bits = 0;
+        __u32 bit_pos;
+	unsigned long flags;
+
+	bit_pos = 1;
+        bit_pos = (1 << (16 * 1 + 0));
+
+	bits |= (((1<<PCI_INTR_RX0_DONE) << 0) | ((1<<PCI_INTR_RX0_END) << 0));
+	bits |= (((1<<PCI_INTR_TX0_DONE) << 0) | ((1<<PCI_INTR_TX0_END) << 0));
+
+        spin_lock_irqsave(&udc->lock, flags);
+
