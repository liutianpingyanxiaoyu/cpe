+static inline void
+ar7240_spi_disable_soft_access(void)
+{
+	ar7240_reg_wr_nf(AR7240_SPI_WRITE, AR7240_SPI_CS_DIS);
+	ar7240_reg_wr_nf(AR7240_SPI_FS, 0);
