+/*
+ * GPIO Misc IRQ Functions
+ */
+void ath_misc_enable_irq(unsigned int mask)
+{
+	ath_reg_rmw_set(ATH_MISC_INT_MASK, mask);
+}
+void ath_misc_disable_irq(unsigned int mask)
+{
+	ath_reg_rmw_clear(ATH_MISC_INT_MASK, mask);
+}
