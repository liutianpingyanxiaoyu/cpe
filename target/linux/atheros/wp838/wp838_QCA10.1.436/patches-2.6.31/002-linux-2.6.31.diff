+char *
+ath_str_to_u(const char *s, uint32_t *u)
+{
+	int	i;
+
+	*u = 0;
+	for (i = 0; isdigit(s[i]) && i < ATH_TEST_TIMER_STR_MAX; i ++) {
+		*u = (*u * 10) + (s[i] - '0');
+	}
+	return s + i;
+}
