gpiomode=`reg p 0x60`

#Set WLED and EPHY pins to GPIO Mode.
reg w 0x60 0xab71d

#Set gpio#40 to output mode.
reg s 0xb0000600
reg w 0x74 0xffd01401

#Set gpio#72 to output mode.
reg w 0x9c 0x1


if [ $1 == "LED" -a $2 == "on" ]; then
	
