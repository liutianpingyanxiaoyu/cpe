+static int __init
+ath_clksw_init(void)
+{
+	u32	tdata;
+
+	printk("%s: Registering Clock Switch Interface ", __func__);
+
+	if ((tdata = misc_register(&ath_clksw_miscdev))) {
+		printk("failed %d\n", tdata);
+		return tdata;
+	} else {
+		printk("success\n");
+	}
+
+	return 0;
+}
+
+late_initcall(ath_clksw_init);
