+    __hif_pci_put_reset(sc);
+    __hif_pci_tgt_reset(sc); 
+
+    __hif_pci_free_intr(sc);
+
+    __hif_pci_flush_engine(sc);
+
+    __hif_pci_free_rxdesc(sc);
+    __hif_pci_free_txdesc(sc);
+
+    adf_os_mem_free(sc);
+}
